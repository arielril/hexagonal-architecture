{
  "name": "hex-ts",
  "version": "1.0.0",
  "description": "Hexagonal typescript project",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:pm2": "pm2-runtime start ./configs/process.yaml",
    "dev": "nodemon ./src/index.ts",
    "build": "rm -rf dist && tsc",
    "test": "jest \\.test\\.ts",
    "test:unit": "jest \\.unit\\.test\\.ts --silent",
    "test:integration": "jest \\.integration\\.test\\.ts",
    "cover": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "migrate:create": "./node_modules/knex/bin/cli.js migrate:make $MIGRATE_NAME --env local",
    "migrate:up": "./node_modules/knex/bin/cli.js migrate:latest --env local"
  },
  "author": "Ariel Ril",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arielril"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@somosphi/logger": "^1.6.0",
    "amqplib": "^0.5.6",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "elastic-apm-node": "^3.5.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "knex": "^0.20.11",
    "moment-timezone": "^0.5.28",
    "mysql2": "^2.1.0",
    "ramda": "^0.26.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.17.0",
    "@types/chance": "^1.0.10",
    "@types/compression": "1.0.1",
    "@types/cors": "^2.8.5",
    "@types/eslint": "^6.8.0",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^16.0.12",
    "@types/helmet": "0.0.45",
    "@types/ioredis": "^4.14.9",
    "@types/jest": "^24.9.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/ramda": "^0.26.44",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "chance": "^1.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^24.9.0",
    "jest-express": "^1.11.3",
    "nock": "^11.9.1",
    "nodemon": "^2.0.3",
    "sinon": "^7.3.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.5.0",
    "typescript": "^3.8.3"
  }
}
